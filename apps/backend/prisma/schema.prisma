generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Quiz {
  id        String   @id @default(uuid())
  title     String
  questions Json // Saving all types of questions
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
